{
  "name": "kak-mode-plus",
  "main": "./lib/kak-mode",
  "version": "1.9.0",
  "description": "Kakoune emulator",
  "keywords": [],
  "repository": "https://github.com/edahlseng/atom-kak-mode-plus",
  "license": "MIT",
  "scripts": {
    "cm": "git-cz",
    "semantic-release": "semantic-release",
    "docs:build": "node docs/generate.js",
    "docs:dev-watch": "nodemon --watch lib --watch docs/*.pug docs/generate.js",
    "docs:dev-serve": "cd docs && reload -b",
    "docs:dev": "concurrently npm:docs:dev-*",
    "link": "cd sources && { [[ -e package.json ]] || ln -s ../package.json; } && { [[ -e node_modules ]] || ln -s ../node_modules; } && apm link",
    "lint": "npm run lint:commit -- --from master; npm run lint:json; npm run lint:js",
    "lint:commit": "commitlint",
    "lint-report:commit": "mkdir -p ./linting-results/commitlint && commitlint --format commitlint-format-junit > ./linting-results/commitlint/report.xml",
    "lint:json": "prettier --config .prettierrc.js --list-different '**/*.json'",
    "lint-report:json": "mkdir -p ./linting-results/prettier-json && prettier-junit --config .prettierrc.js '**/*.json' > ./linting-results/prettier-json/report.xml",
    "lint:js": "eslint ./",
    "lint-report:js": "eslint ./ --output-file ./linting-results/eslint/report.xml --format junit"
  },
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  },
  "atomTestRunner": "./spec/custom-runner",
  "dependencies": {
    "escape-string-regexp": "^1.0.5",
    "ramda": "^0.26.1"
  },
  "devDependencies": {
    "@eric.dahlseng/configuration-lint": "^0.4.2",
    "@semantic-release/apm": "^1.0.0",
    "@semantic-release/changelog": "^3.0.1",
    "@semantic-release/git": "^7.0.5",
    "atom-jasmine3-test-runner": "^4.1.1",
    "commitizen": "^3.0.5",
    "concurrently": "^4.1.0",
    "cz-conventional-changelog": "^2.1.0",
    "documentation": "^9.0.0",
    "markdown-it": "^8.4.2",
    "nodemon": "^1.18.9",
    "pug": "^2.0.3",
    "reload": "^2.4.0",
    "semantic-release": "^15.13.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
